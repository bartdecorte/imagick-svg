{
    "name": "bartdecorte/imagick-svg",
    "description": "Render SVGs using Imagick",
    "homepage": "https://github.com/bartdecorte/imagick-svg",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.0",
        "ext-imagick": "^3.6",
        "ext-xmlreader": "*"
    },
    "authors": [
        {
            "name": "Bart Decorte",
            "email": "bart@bartdecorte.be"
        }
    ],
    "autoload": {
        "psr-4": {
            "BartDecorte\\ImagickSvg\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "BartDecorte\\ImagickSvg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox --coverage-text",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.7",
        "phpunit/phpunit": "^9.5"
    }
}
